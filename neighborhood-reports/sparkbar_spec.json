{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "mark": {
        "type": "bar"
    },
    "datasets": {
        "the_data": []
    },
    "data": {
        "name": "the_data"
    },
    "config": {
        "axis": {
            "grid": false
        },
        "background": "transparent",
        "view": {
            "continuousHeight": 300,
            "continuousWidth": 300,
            "strokeWidth": 0
        }
    },
    "encoding": {
        "color": {
            "condition": {
                "test": "datum.neighborhood == 'Kingsbridge - Riverdale'",
                "value": "#00923E"
            },
            "value": "#D2D4CE"
        },
        "x": {
            "axis": null,
            "field": "neighborhood",
            "sort": "y",
            "type": "nominal"
        },
        "y": {
            "axis": null,
            "field": "unmodified_data_value_geo_entity",
            "type": "quantitative"
        }
    },
    "height": 100,
    "width": 300
}